   "source": [
    "def jump_game(nums):\n",
    "    target = len(nums)-1\n",
    "    for i in range(len(nums)-1,-1,-1):\n",
    "        if i + nums[i] >= target:\n",
    "            target = i\n",
    "    return target == 0"
   ]
  },
