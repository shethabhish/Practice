    "def max_profit(prices):\n",
    "    ans = 0\n",
    "    for i in range(len(prices)-1):\n",
    "        if prices[i+1] >prices[i]:\n",
    "            ans += prices[i+1] - prices[i]\n",
    "    return ans"
   ]
