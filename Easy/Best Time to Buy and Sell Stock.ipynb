    "def max_profit(prices):\n",
    "    if len(prices) == 0:\n",
    "        return 0\n",
    "    minimum,maxdiff = float(\"inf\"),0\n",
    "    for i in range(len(prices)):\n",
    "        minimum = min(minimum,prices[i])\n",
    "        maxdiff = max(prices[i]-minimum,maxdiff)\n",
    "    return maxdiff"
   ]
