   "source": [
    "def house_robber(nums):\n",
    "    prevmax,currmax = 0,0\n",
    "    for i in nums:\n",
    "        temp = currmax\n",
    "        currmax = max(currmax,prevmax+i)\n",
    "        prevmax = temp\n",
    "    return currmax"
   ]
  },
