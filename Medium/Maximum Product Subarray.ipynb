   "source": [
    "def max_product(nums):\n",
    "    maxi = big = small = nums[0]\n",
    "    for i in nums[1:]:\n",
    "        big,small = max(i,i*big,i*small),min(i,i*big,i*small)\n",
    "        maxi = max(maxi,big)\n",
    "    return maxi"
   ]
  },
