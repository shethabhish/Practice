    "def paint_fence(n,k):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    same,diff = 0,k\n",
    "    for i in range(2,n+1):\n",
    "        same,diff = diff,(same+diff)*(k-1)\n",
    "    return same+diff"
   ]
  },
