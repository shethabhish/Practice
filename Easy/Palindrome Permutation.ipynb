   "source": [
    "from collections import Counter\n",
    "def palindrome_partition(s):\n",
    "    freq = Counter(s)\n",
    "    count  = 0\n",
    "    for ch,fr in freq.items():\n",
    "        if fr == 1:\n",
    "            count += 1\n",
    "    return count <= 1"
   ]
  },
