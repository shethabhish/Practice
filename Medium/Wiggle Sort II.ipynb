   "source": [
    "def wiggle_sort(nums):\n",
    "    nums.sort()\n",
    "    half = len(nums[::2])\n",
    "    nums[::2],nums[1::2] = nums[:half][::-1],nums[half:][::-1]\n",
    "    return nums"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
